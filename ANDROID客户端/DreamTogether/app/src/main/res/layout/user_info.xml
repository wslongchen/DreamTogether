<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dreamBackground">
    <!--style="@style/TextSettingItem"-->
    <ListView

        android:id="@+id/user_info_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@mipmap/line"
        android:listSelector="@color/dreamBlack"
        android:layout_alignParentStart="true">

    </ListView>

    <com.example.mrpan.dreamtogether.view.TitleBar
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:id="@+id/top_bar"
        android:layout_alignParentStart="true" />
</RelativeLayout>
</RelativeLayout>